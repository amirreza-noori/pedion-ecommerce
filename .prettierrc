{
  "semi": true,
  "trailingComma": "all",
  "singleQuote": true,
  "printWidth": 120,
  "tabWidth": 2,
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^@?(react|next).*$",
    "^@/lib.*$",
    "^@/apis.*$",
    "^(@/interfaces.*)|(.*\\.d\\.ts)|(.*[tT]ypes?)|(.*/[tT]ypes?/.*)$",
    "^.*$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
